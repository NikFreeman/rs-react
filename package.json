{
  "name": "rs-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve": "ts-node server.ts",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "build": "npm run build:client && npm run build:server",
    "dev": "vite",
    "test:cypress": "start-server-and-test serve http://localhost:7456/@vite/client cypress",
    "cypress:coverage": "start-server-and-test serve http://localhost:7456/@vite/client cy:e2e",
    "cypress:run": "cypress run",    
    "cy:e2e": "npx cypress run --e2e --env coverage=true && npx nyc report --reporter=text-summary",
    "build-old": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts --ext .tsx",
    "lint:fix": "eslint ./src --ext .ts --ext .tsx --fix",
    "test": "jest",    
    "coverage": "jest --watchAll --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@hookform/devtools": "^4.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.1.2",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1",
    "whatwg-fetch": "^3.6.2"
  }
}
